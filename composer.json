{
    "name": "whitecube/laravel-sluggable",
    "description": "Generate slugs for your models automatically - even with translated attributes",
    "keywords": [
        "whitecube",
        "laravel-sluggable"
    ],
    "homepage": "https://github.com/whitecube/laravel-sluggable",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Adrien Leloup",
            "email": "adrien@whitecube.be",
            "role": "Developer"
        },
        {
            "name": "Toon Van den Bos",
            "email": "toon@whitecube.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^9.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "laravel/helpers": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "spatie/laravel-translatable": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Whitecube\\Sluggable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Whitecube\\Sluggable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
